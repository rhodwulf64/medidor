                        </label>
                    </div>
                    <div class="col-xs-12">
                        <span style="white-space: pre-line;">{{Alergias}}</span>
                    </div>
                {{/if}}
                
                {{#if Cirugias}}
                    <div class="col-xs-12"style="margin-top:10px;">
