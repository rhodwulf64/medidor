                {{/if}}
                
                {{#if Sedentarismo}}
                    <div class="col-xs-4">
                        <label>Sedentarismo:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleSedentarismo}}</span>
                        </div>
                    </div>
                {{/if}}
                
                
                {{#if ActividadFisica}}
                    <div class="col-xs-4">
                        <label>Actividad Física:</label>
                        <div class="col-md-12">
