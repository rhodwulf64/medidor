                    </div>
                    <div class="col-xs-4">
                        <label>Cesareas:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{Cesareas}}</span>
                        </div>
                    </div>
                    <div class="col-xs-4">
                        <label>HijosVivos:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{HijosVivos}}</span>
                        </div>
                    </div>
            
                
                {{#if Perinatales}}
                    <div class="col-xs-12" style="margin-top:10px;">
                        <label>
                            Antecedentes Perinatales:
