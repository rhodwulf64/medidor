<html>
    <head>    
        <meta charset="utf-8" />
        <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
        <script src="https://webmedico.net/Scripts/moment.js"></script>
        <script src="https://webmedico.net/Scripts/moment-with-locales.js"></script>
        <style>
            body {
                font: 11px "Tahoma" !important;
            }
            
            hr {
                border-top-color: #{{ColorLines}} !important;
            }
            
        </style>
    </head>
    <body>
        <span>&nbsp;</span>
        
        <div class="row">
            <div class="col-xs-6">
                <h4>Antecedentes Personales</h4>
            </div>
            <div class="col-xs-6" style="padding-top:15px">
                <span class="pull-right"><strong>Fecha:&nbsp; <span id="DatePrint"></span></strong></span>
            </div>
        </div>
        <hr>
        <div width="100%">
                {{#unless PrinterHeader}}
                <div><h5><strong>Doctor:</strong>&nbsp; {{DoctorName}} </h5></div>
                {{/unless}}
                <div><h5><strong>Paciente:</strong>&nbsp; {{PatientName}} </h5></div>
                <hr>
        </div>
            
            <div class="row">
                {{#if Diabetes}}
                    <div class="col-xs-4">
                        <label>Diabetes:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleDiabetes}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if Hipertension}}
                    <div class="col-xs-4">
                        <label>Hipertensión:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleHipertension}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if Cancer}}
                    <div class="col-xs-4">
                        <label>Cancer:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleCancer}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if Neurologica}}
                    <div class="col-xs-4">
                        <label>Enf. Neurológica:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleNeurologica}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if Cardiologica}}
                    <div class="col-xs-4">
                        <label>Enf. Cardiológica:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleCardiologica}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if Gastroinstestinal}}
                    <div class="col-xs-4">
                        <label>Enf. Gastroinstestinal:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleGastroinstestinal}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if Infecciosas}}
                    <div class="col-xs-4">
                        <label>Enf. Infecciosas:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleInfecciosas}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if EnfermedadTiroidea}}
                    <div class="col-xs-4">
                        <label>Enf. Tiroidea:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleEnfermedadTiroidea}}</span>
                        </div>
                    </div>
                {{/if}}
                
                {{#if Respiratoria}}
                    <div class="col-xs-4">
                        <label>Enf. Respiratoria:</label>
                        <div class="col-md-12">
                            <span style="white-space: pre-line;">{{DetalleRespiratoria}}</span>
                        </div>
                    </div>
                {{/if}}
                
                
                

                
                
                
                
            </div>

            <div class="row">
                {{#if Patologicos}}
                    <div class="col-xs-12" style="margin-top:10px;">
                        <label>
                            Antecedentes Patológicos:
                        </label>
                    </div>
                    <div class="col-xs-12">
                        <span style="white-space: pre-line;">{{Patologicos}}</span>
                    </div>
                {{/if}}
                
                {{#if Medicamentos}}
                    <div class="col-xs-12" style="margin-top:10px;">
                        <label>
                            Medicamentos:
                        </label>
                    </div>
                    <div class="col-xs-12">
                        <span style="white-space: pre-line;">{{Medicamentos}}</span>
                    </div>
                {{/if}}
                
              
                    <div class="col-xs-12" style="margin-top:10px;">
                        <label>
                            Hábitos:
                        </label>
                    </div>
                    <br/>
                  {{#if Fuma}}
