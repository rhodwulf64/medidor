<html>    <head>            <meta charset="utf-8" />        <script src="https://webmedico.net/Scripts/moment.js"></script>        <script src="https://webmedico.net/Scripts/moment-with-locales.js"></script>        <style>            body {                font: 11px "Tahoma";                padding-left:20px;                padding-right:20px;            }                        .table {                font: 11px "Tahoma";                /*border: 1px solid;*/            }                        .table, th, td {                border: 1px solid !important;                                border-collapse: collapse;            }                    .BG-Green {                text-align: center;                background-color: #c2e4cd !important;                -webkit-print-color-adjust: exact;            }                    .BG-Yellow {                background-color: #f8ef9f !important;                -webkit-print-color-adjust: exact;            }                    .BG-Pink {                text-align: left;                background-color: #ef9ebc !important;                -webkit-print-color-adjust: exact;            }                            </style>    </head>    <body>        <table width="100%" class="table">            <thead class="BG-Green">                <tr>                    <th width="20%">ESTABLECIMIENTO</th>                    <th width="20%">NOMBRE</th>                    <th width="20%">APELLIDO</th>                    <th width="10%">                        <div style="min-width:100%; border-bottom:1px solid;">SEXO</div>                        <div style="min-width:49%; max-width:49%; float:left; border-right:1px solid;">M</div><div style="min-width:49%;float:left;margin:0">F</div>                    </th>                    <th width="10%">NUMERO DE HOJA</th>                    <th width="20%">HISTORIA CLINICA</th>                </tr>            </thead>            <tbody>                <tr style="height: 5mm;">                    <td>&nbsp;</td>                    <td style="text-align:center;">{{Soap.Paciente.FirstName}} {{Soap.Paciente.SecondFirstName}}</td>                    <td style="text-align:center;">{{Soap.Paciente.LastName}} {{Soap.Paciente.SecondLastName}}</td>                    <td  class="BG-Yellow" style="text-align:center;"><div style="min-width:49%; max-width:49%; float:left; border-right:1px solid;">{{{SexoM Soap.Paciente.Sexo }}}</div><div style="min-width:49%;float:left;margin:0">{{{SexoF Soap.Paciente.Sexo }}}</div></td>                    <td>&nbsp;</td>                    <td>&nbsp;</td>                </tr>            </tbody>        </table>        <table width="100%" class="table" style="margin-top: 5px; table-layout:fixed;">            <thead class="BG-Pink">                <tr style="height: 5mm;">                    <th style="padding-left:10px;">1   MOTIVO DE CONSULTA</th>                </tr>            </thead>            <tbody>                <tr style="height: 5mm;">                    <td style="padding-left:10px;">                        <div style="max-height: 5mm; overflow:hidden; text-overflow:ellipsis;">                            {{#if Soap.Motivo}}                                {{Soap.Motivo}}                            {{/if}}                        </div>                    </td>                </tr>            </tbody>        </table>        <table width="100%" class="table" style="margin-top: 5px;">            <thead class="BG-Pink">                <tr>                    <th>&nbsp;&nbsp;2   ANTECEDENTES PERSONALES</th>                </tr>            </thead>            <tbody>                <tr>                    <td style="padding-left:10px; height: 15mm !important;">                         <div style="max-height: 15mm; overflow:hidden; text-overflow:ellipsis;">                            {{#if Soap.Personales.Diabetes}}                                <strong>Diabetes:</strong> {{Soap.Personales.DetalleDiabetes}}.                            {{/if}}                            {{#if Soap.Personales.Hipertension}}                                <strong>Hipertensión:</strong> {{Soap.Personales.DetalleHipertension}}.                            {{/if}}                            {{#if Soap.Personales.Cancer}}                                <strong>Cancer:</strong> {{Soap.Personales.DetalleCancer}}.                            {{/if}}                            {{#if Soap.Personales.Fuma}}                                <strong>Fuma:</strong> {{Soap.Personales.DetalleFuma}}.                            {{/if}}                            {{#if Soap.Personales.Alcohol}}                                <strong>Alcohol:</strong> {{Soap.Personales.DetalleAlcohol}}.                            {{/if}}                            {{#if Soap.Personales.Infecciosas}}                                <strong>Infecciosas:</strong> {{Soap.Personales.DetalleInfecciosas}}.                            {{/if}}                            {{#if Soap.Personales.EnfermedadTiroidea}}                                <strong>Enf. Tiroidea:</strong> {{Soap.Personales.DetalleEnfermedadTiroidea}}.                            {{/if}}                            {{#if Soap.Personales.Asma}}                                <strong>Asma:</strong> {{Soap.Personales.DetalleAsma}}.                            {{/if}}                            {{#if Soap.Personales.RinitisAlergica}}                                <strong>Rinitis Alérgica:</strong> {{Soap.Personales.DetalleRinitisAlergica}}.                            {{/if}}                        </div>                    </td>                </tr>            </tbody>        </table>        <table width="100%" class="table" style="margin-top: 5px;">            <thead class="BG-Pink">                <tr>                    <th>&nbsp;&nbsp;3   ANTECEDENTES FAMILIARES</th>                </tr>            </thead>            <tbody>                <tr>                    <td style="padding-left:10px; height: 15mm !important;">                        <div style="max-height: 15mm; overflow:hidden; text-overflow:ellipsis;">                            {{#if Soap.Familiares.Diabetes}}                                <strong>Diabetes:</strong> Si.                            {{/if}}                            {{#if Soap.Familiares.Hipertension}}                                <strong>Hipertensión:</strong> Si.                            {{/if}}                            {{#if Soap.Familiares.Cancer}}                                <strong>Cancer:</strong> Si.                            {{/if}}                            {{#if Soap.Familiares.CerebroVascular}}                                <strong>Accidente Cerebrovascular:</strong> Si.                            {{/if}}                            {{#if Soap.Familiares.EnfermadadCardiaca}}                                <strong>Enfermedad Cardíaca:</strong> Si.                            {{/if}}                            {{#if Soap.Familiares.Hipoacusia}}                                <strong>Hipoacusia:</strong> Si.                            {{/if}}                            {{#if Soap.Familiares.Comentarios}}                                <strong>Comentarios:</strong> {{Soap.Familiares.Comentarios}}.                            {{/if}}                        </div>                    </td>                </tr>            </tbody>        </table>        <table width="100%" class="table" style="margin-top: 5px;">            <thead class="BG-Pink">                <tr>                    <th>&nbsp;&nbsp;4   ENFERMEDAD O PROBLEMA ACTUAL</th>                </tr>            </thead>            <tbody>                <tr>                    <td style="padding-left:10px; height: 35mm !important;padding-right:10px;">&nbsp;                        <div style="max-height: 35mm; overflow:hidden; text-overflow:ellipsis;">                            {{Soap.Subjetivo}}                        </div>                    </td>                </tr>            </tbody>        </table>            <table width="100%" class="table" style="margin-top: 5px;">                <thead class="BG-Pink">                    <tr>                        <th>&nbsp;&nbsp;5    REVISION ACTUAL DE ORGANOS Y SISTEMAS</th>                    </tr>                </thead>                <tbody>                    <tr>                        <td style="height: 15mm !important;">&nbsp;</td>                    </tr>                </tbody>            </table>            <table width="100%" class="table" style="margin-top: 5px;">                <thead class="BG-Pink">                    <tr>                        <th colspan="9">&nbsp;&nbsp;6   SIGNOS VITALES</th>                    </tr>                </thead>                <tbody>                    <tr style="height: 5mm;">                        <td class="BG-Green" width="20%">FECHA</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                    </tr>                    <tr style="height: 5mm;">                        <td class="BG-Green" width="20%">PRESION ARTERIAL</td>                        <td style="text-align:center" width="10%">{{Soap.TA}}</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                    </tr>                    <tr style="height: 5mm;">                        <td class="BG-Green" width="20%">PULSO X min</td>                        <td style="text-align:center" width="10%">{{Soap.FC}}</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                    </tr>                    <tr style="height: 5mm;">                        <td class="BG-Green" width="20%">TEMPERATURA ºC</td>                        <td style="text-align:center" width="10%">{{Soap.Temp}}</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                        <td>&nbsp;</td>                    </tr>                </tbody>            </table>            <table width="100%" class="table" style="margin-top: 5px;">                <thead class="BG-Pink">                    <tr>                        <th>&nbsp;&nbsp;7    EXAMEN FISICO</th>                    </tr>                </thead>                <tbody>                    <tr>                        <td style="padding-left:10px; height: 40mm !important;padding-right:10px;">&nbsp;                            <div style="max-height: 40mm; overflow:hidden; text-overflow:ellipsis;">                              <div class="col-md-12">                                    {{#if SoapObjetivo}}                                                <label style="font-weight: bold;" >                                                    Oído Derecho:                                                </label>                                                {{#if SoapObjetivo.OidoDerechoNormal}}                                                <label>Normal</label>                                                {{else}}                                                <label>Anormal: {{SoapObjetivo.DetalleOidoDerecho}}</label>                                                {{/if}}<br/>                                               <label style="font-weight: bold;" >                                                    Oído Izquierdo:                                                </label>                                                 {{#if SoapObjetivo.OidoIzquierdoNormal}}                                                <label>Normal</label>                                                {{else}}                                                <label>Anormal: {{SoapObjetivo.DetalleOidoDerecho}}</label>                                                {{/if}}<br/>                                                <label style="font-weight: bold;" >                                                    Nariz:                                                </label>                                                {{#if SoapObjetivo.NarizNormal}}                                                    <label>Normal </label>                                                {{else}}                                                    <label>Anormal: {{SoapObjetivo.DetalleNariz}}</label>                                                {{/if}}<br/>                                                <label style="font-weight: bold;" >                                                    OroFaringe:                                                </label>                                                 {{#if SoapObjetivo.FaringeNormal}}                                                <label>Normal</label>                                                {{else}}                                                <label>Anormal: {{SoapObjetivo.DetalleFaringe}}</label>                                                {{/if}}<br/>                                                <label style="font-weight: bold;" >                                                    Cuello:                                                </label>                                                 {{#if SoapObjetivo.CuelloNormal}}                                                <label>Normal</label>                                                {{else}}                                                <label>Anormal: {{SoapObjetivo.DetalleCuello}}</label>                                                {{/if}}<br/>                                                <label style="font-weight: bold;" >                                                    Pulmones:                                                </label>                                                 {{#if SoapObjetivo.PulmonesNormal}}                                                <label>Normal</label>                                                {{else}}                                                <label>Anormal: {{SoapObjetivo.DetallePulmones}}</label>                                                {{/if}}<br/>                                                <label style="font-weight: bold;" >                                                    Detalle Otros:                                                </label>                                           <div>                                              {{/if}}                                                                                                                                  {{#if Soap.Objetivo}}                                            <span style="white-space: pre-line">{{Soap.Objetivo}}</span>                                             {{else}}                                                 <span style="white-space: pre-line">{{SoapObjetivo.DetalleOtros}}</span>                                            {{/if}}                                          </div>                                                                                     </div>                            </div>                        </td>                    </tr>                </tbody>            </table>            <table width="100%" class="table" style="margin-top: 5px;">                <thead class="BG-Pink">                    <tr>                        <th width="50%" style="text-align:center;">                            <div style="max-height: 5mm; overflow:hidden; text-overflow:ellipsis;">                                <span>&nbsp;&nbsp;8 DIAGNOSTICOS</span>                                <span style="min-width:35px;max-width:35px;float:right;border-left:1px solid;">DEF</span>                                <span style="min-width:42px;max-width:42px;float:right;border-left:1px solid;">PRE</span>                                <span style="min-width:52px;max-width:52px;float:right;border-left:1px solid;">CIE</span>                            </div>                        </th>                        <th width="50%" style="text-align:center;">                            <div style="max-height: 5mm; overflow:hidden; text-overflow:ellipsis;">                                <span style="font-size:9px!important">PRE= PRESUNTIVO DEF= DEFINITIVO</span>                                <span style="min-width:35px;max-width:35px;float:right;border-left:1px solid;">DEF</span>                                <span style="min-width:42px;max-width:42px;float:right;border-left:1px solid;">PRE</span>                                <span style="min-width:52px;max-width:52px;float:right;border-left:1px solid;">CIE</span>                            </div>                        </th>                    </tr>                </thead>                <tbody>                    <tr style="height: 5mm;">                        <td style="text-align:center;">                            <div style="max-height: 5mm; overflow:hidden; text-overflow:ellipsis;">                                <span>                                    <span class="BG-Green" style="min-width:15px;max-width:15px;float:left;border-right:1px solid;">1</span>                                    <span style="font-size: 9px !important;max-width:49mm !important; max-height: 5mm !important;display: inline-block; white-space:nowrap; overflow:hidden !important; text-overflow:ellipsis !important;">{{Soap.CieCodes.0.Descriptor}}</span>                                </span>                                 <span class="BG-Yellow" style="min-width:35px;max-width:35px;float:right;border-left:1px solid;">{{#if Diagnositcos.0.presuntivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                                <span class="BG-Yellow" style="min-width:42px;max-width:42px;float:right;border-left:1px solid;">{{#if Diagnositcos.0.definitivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                               <span style="min-width:52px;max-width:52px;float:right;border-left:1px solid;">{{#if Soap.CieCodes.0.Codigo}}{{Soap.CieCodes.0.Codigo}}{{else}}&nbsp;{{/if}}</span>                            </div>                        </td>                        <td style="text-align:center;">                            <div style="max-height: 5mm; overflow:hidden; text-overflow:ellipsis;">                                <span>                                    <span class="BG-Green" style="min-width:15px;max-width:15px;float:left;border-right:1px solid;">3</span>                                    <span style="font-size: 9px !important;max-width:49mm !important; max-height: 5mm !important;display: inline-block; white-space:nowrap; overflow:hidden !important; text-overflow:ellipsis !important;">{{Soap.CieCodes.2.Descriptor}}</span>                                </span>                                <span class="BG-Yellow" style="min-width:35px;max-width:35px;float:right;border-left:1px solid;">{{#if Diagnositcos.2.presuntivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                                <span class="BG-Yellow" style="min-width:42px;max-width:42px;float:right;border-left:1px solid;">{{#if Diagnositcos.2.definitivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                                <span style="min-width:52px;max-width:52px;float:right;border-left:1px solid;">{{#if Soap.CieCodes.2.Codigo}}{{Soap.CieCodes.2.Codigo}}{{else}}&nbsp;{{/if}}</span>                            </div>                        </td>                    </tr>                    <tr style="height: 5mm;">                        <td style="text-align:center;">                            <div style="max-height: 5mm; overflow:hidden; text-overflow:ellipsis;">                                <span>                                    <span class="BG-Green" style="min-width:15px;max-width:15px;float:left;border-right:1px solid;">2</span>                                    <span style="font-size: 9px !important;max-width:49mm !important; max-height: 5mm !important;display: inline-block; white-space:nowrap; overflow:hidden !important; text-overflow:ellipsis !important;">{{Soap.CieCodes.1.Descriptor}}</span>                                </span>                                 <span class="BG-Yellow" style="min-width:35px;max-width:35px;float:right;border-left:1px solid;">{{#if Diagnositcos.1.presuntivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                                <span class="BG-Yellow" style="min-width:42px;max-width:42px;float:right;border-left:1px solid;">{{#if Diagnositcos.1.definitivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                               <span style="min-width:52px;max-width:52px;float:right;border-left:1px solid;">{{#if Soap.CieCodes.1.Codigo}}{{Soap.CieCodes.1.Codigo}}{{else}}&nbsp;{{/if}}</span>                            </div>                        </td>                        <td style="text-align:center;">                            <div style="max-height: 5mm; overflow:hidden; text-overflow:ellipsis;">                                <span>                                    <span class="BG-Green" style="min-width:15px;max-width:15px;float:left;border-right:1px solid;">4</span>                                    <span style="font-size: 9px !important; max-width:49mm !important; max-height: 5mm !important;display: inline-block; white-space:nowrap; overflow:hidden !important; text-overflow:ellipsis !important;">{{Soap.CieCodes.3.Descriptor}}</span>                                </span>                                <span class="BG-Yellow" style="min-width:35px;max-width:35px;float:right;border-left:1px solid;">{{#if Diagnositcos.3.presuntivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                                <span class="BG-Yellow" style="min-width:42px;max-width:42px;float:right;border-left:1px solid;">{{#if Diagnositcos.3.definitivo}}<span>✓</span>{{else}}&nbsp;{{/if}}</span>                                <span style="min-width:52px;max-width:52px;float:right;border-left:1px solid;">{{#if Soap.CieCodes.3.Codigo}}{{Soap.CieCodes.3.Codigo}}{{else}}&nbsp;{{/if}}</span>                            </div>                        </td>                    </tr>                </tbody>            </table>            <table width="100%" class="table" style="margin-top: 5px;">                <thead class="BG-Pink">                    <tr>                        <th>&nbsp;&nbsp;9    PLANES</th>                    </tr>                </thead>                <tbody>                    <tr>                        <td style="padding-left:10px; height: 30mm !important;padding-right:10px;">&nbsp;                            <div style="max-height: 30mm; overflow:hidden; text-overflow:ellipsis;">                                {{Soap.Plan}}                            </div>                        </td>                    </tr>                </tbody>            </table>            <table width="100%" class="table" style="margin-top: 5px;">                <tr>                    <td class="BG-Green" style="font: bold 8px tahoma !important;" width="8%">FECHA</td>                    <td width="10%" style="font: bold 9px tahoma !important;text-align:center;"><span id="Fecha"></span></td>                    <td class="BG-Green" style="font: bold 8px tahoma !important" width="5%">HORA FIN</td>                    <td width="7%" style="font: bold 9px tahoma !important;text-align:center;"><span id="hora"></span></td>                    <td class="BG-Green" style="font: bold 8px tahoma !important;" width="5%">MEDICO</td>                    <td style="padding-left: 5px">{{Soap.DoctorName}}</td>                    <td width="7%">&nbsp;</td>                    <td class="BG-Green" style="font: bold 8px tahoma !important" width="5%">FIRMA</td>                    <td width="23%">&nbsp;</td>                </tr>            </table>    </body></html><script>    var fecha = moment().format('DD/MM/YYYY');    var hora = moment().format('HH:mm');    document.getElementById("Fecha").innerHTML = fecha;    document.getElementById("hora").innerHTML = hora;</script>